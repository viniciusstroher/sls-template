{
  "name": "sls-template",
  "version": "1.0.0",
  "description": "serverless create --template saipos-nodejs-ts --path .",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dependencies-layer": "node build/build-dependencies-layer.js",
    "build-utils-layer": "npm run build-tsc && node build/build-utils-layer.js",
    "build-tsc": "node node_modules/typescript/bin/tsc",
    "deploy-dev": "npm run build-tsc && npm run build-utils-layer && sls deploy -s dev --aws-profile teste",
    "deploy-offline": "npm run build-tsc && npm run build-utils-layer && set SLS_DEBUG=* && set AWS_PROFILE=teste && serverless offline start --useDocker -r us-west-2 --stage dev",
    "docker-local-sqs": "docker-compose up"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.437.0",
    "request-promise": "^4.2.6",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "package-lock.json": "^1.0.0",
    "serverless-offline": "^7.0.0",
    "serverless-offline-localstack": "0.0.3",
    "serverless-offline-sqs": "^4.1.1",
    "serverless-offline-aws-sqs": "^1.0.3",
    "serverless-webpack": "^5.5.0",
    "webpack": "^5.38.1",
    "fs-extra": "^10.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
